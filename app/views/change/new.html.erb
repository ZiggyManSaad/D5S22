          </div>
          <div class="form-group">
              <%= f.label :drugTestRCVD %>
              <%= check_box_tag(:drugTestRCVD) %>
          </div>
          <div class="form-group">
              <%= f.label :review %>
              <%= check_box_tag(:review) %>
          </div>
          <div class="form-group">
              <%= f.label :retire401k %>
              <%= check_box_tag(:retire401k) %>
          </div>
          <div class="form-group">
