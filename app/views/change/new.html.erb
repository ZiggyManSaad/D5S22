<div class="container">
  <div class="row">
    <h3 class="text-center">Change Of Employment</h3>
    <div class="col-md-4 col-md-offset-4">
      
      <div class="well">
        <%= form_for @change do |f| %>
          <div class="form-group">
              <%= f.label :name %>
              <%= f.text_field :name, class: 'form-control' %>
          </div>
          <div class="form-group">
              <%= f.label :dateToday %>
              <%= f.text_field :dateToday, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :dateCOE %>
              <%= f.text_field :DateCOE, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :ADP %>
              <%= f.text_field :ADP, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :typeCOE %>
              <%= f.text_field :typeCOE, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :fromTeam %>
              <%= f.text_field :fromTeam, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :fromPos %>
              <%= f.text_field :fromPos, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :toTeam %>
              <%= f.text_field :toTeam, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :toPos %>
              <%= f.text_field :toPos, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :teamLead %>
              <%= f.text_field :teamLead, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :access %>
              <%= f.text_field :access, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :equipment %>
              <%= f.text_field :equipment, class: 'form-control'  %>
          </div>
          <!-- Insert HR Checklist boxes here -->
          <div class="form-group">
              <%= f.label :empStatusEmail %>
              <%= check_box_tag(:empStatusEmail) %>
          </div>
          <div class="form-group">
              <%= f.label :tlList %>
              <%= check_box_tag(:tlList) %>
          </div>
          <div class="form-group">
