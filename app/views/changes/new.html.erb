<div class="container">
  <div class="row">
    <h3 class="text-center"><strong>Change Of Employment</strong></h3>
    <div class="col-lg-8 col-md-offset-2">
      
      <div class="well">
        <%= form_for @change do |f| %>
        
        <% @team_array = [ ['Administration Department (301810)'], ['Arthrex (301233)'], [ 'Finance Department (301820)'], [ 'Flexibles (301250)'], [ 'Hologic (301260)'], [ 'Human Resources (301840)'], [ 'IT Department (301850)'], [ 'Maintenance (301919)'], [ 'Materials (301110)'], [ 'Process Engineering (301350)'], [ 'Production Overhead (301310)'], [ 'Production Services (301290)'], [ 'Quality Department (301340)'], [ 'Sales & Marketing (301720)'], [ 'Shipping (301740)'], [ 'SNE New (301221)'], [ 'SNE Team 2 (301222)'], [ 'Stryker Team 1 (301211)'], [ 'Stryker Team 2 (301212)'], [ 'Stryker Team 3 (301231)'], [ 'Thermohauser (680680)'], [ 'Thermohauser (700700)'], [ 'Vet. Syringe (301730)'], [ 'Vetline (301240)'] ] %>
        <% @position_array = [ ['Accounting & Customer Service Support'], ['Accounting Manager'], ['Buyer/ Planner'], ['CNC Programmer-Tool & Fix Coord'], ['Couplers'], ['Customer Service Supervisor'], ['Data Entry'], ['Deputy'], ['Deputy- Finalist'], ['Deputy- Stockroom'], ['Director of Finance And Controlling'], ['Director of Production'], ['Director Of Quality, ASQ CQE & CMQ/OE'], ['Director Of Sales & Customer Service'], ['Disassembler/Allocator'], ['Engineering Intern'], ['Engineering Manager'], ['Eval'], ['Evaluation Assistant Coordinator'], ['Final QC Inspector'], ['Finalist'], ['Human Resources Coordinator'], ['Human Resources Generalist'], ['Human Resources Manager'], ['Inner Tubes'], ['Inner Tubes Coordinator'], ['Inner Tubes/Final'], ['IT Manager'], ['IT Support Specialist'], ['Janitor'], ['Lead Design Engineer'], ['Logistics Flow Team Member'], ['Machinist Intern'], ['Maintenance Manager & Safety Officer'], ['Maintenance Technician'], ['Maintenance Technician/Facilities'], ['Mechanical Designer'], ['OT Coordinator'], ['Outer Tubes'], ['Process Development Tech/Trainer TL'], ['Process Development Technician'], ['Production Scheduler & Metrics Analyst'], ['Quality Assurance Coordinator'], ['Quality Control Coordinator'], ['Quality Technician'], ['Receptionist'], ['Salvage'], ['Senior Assembly Technician'], ['Stockroom Clerk'], ['Team Deputy- Logistics Flow'], ['Team Leader'], ['Teamleader- Finalist'], ['Teamleader- Logistics Flow'], ['Teamleader- Salvage'], ['Teamleader- Stockroom Clerk'], ['Teamleader- Welding'], ['Tool Crib Attendant'], ['Tooling & Fixtures Maintenance Tech'], ['Vice President Of Operations'], ['VP of Sales'], ['Welder'] ] %>
        
          <div class="form-group">
              <%= f.label :name, "Name" %>
              <%= f.text_field :name, class: 'form-control' %>
          </div>
          <div class="form-group">
              <%= f.label :dateToday, "Today's Date" %>
              <%= f.date_field :dateToday, value: Date.today, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :dateCOE, "Date of COE" %>
              <%= f.date_field :dateCOE, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :ADP, "ADP" %>
              <%= f.text_field :ADP, class: 'form-control'  %>
          </div>
          
          <div class="form-group">
              <%= f.label :typeCOE, "Type of COE" %>
              <%= f.select :typeCOE, ['New Hire', 'Transfer', 'Termination'], include_blank: true, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :fromTeam, "From Team" %>
              <%= f.select :fromTeam, @team_array, include_blank: true, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :fromPos, "From Position" %>
              <%= f.select :fromPos, @position_array, include_blank: true, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :toTeam, "To Team" %>
              <%= f.select :toTeam, @team_array, include_blank: true, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :toPos, "To Position" %>
              <%= f.select :toPos, @position_array, include_blank: true, class: 'form-control'  %>
          </div>
          <div class="form-group">
              <%= f.label :teamLead, "Team Lead" %>
              <%= f.text_field :teamLead, class: 'form-control'  %>
          </div>
          <div class="form-group well">
              <%= f.label :access, "Access Needed" %>
              <br>
              <%= f.check_box :activedirectoy, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :activedirectoy, "PC Log On" %>
              &ensp;
              <%= f.check_box :email, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :email, "Email Access" %>
              &ensp;
              <%= f.check_box :SAPaccess, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :SAPaccess, "SAP Access" %>
              &ensp;
              <%= f.check_box :toolsfixtures, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :toolsfixtures, "Tools and Fixtures" %>
          </div>
          <!-- Equipment needed -->
          <div class="form-group well">
            <%= f.label :equipment, "Equipment Needed" %>
            <br>
            
              <%= f.check_box :desktop, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :desktop, "Desktop" %>
              &ensp;
              <%= f.check_box :Laptop, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :Laptop, "Laptop" %>
              &ensp;
              <%= f.check_box :deskphone, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :deskphone, "Desk Phone" %>
              &ensp;
              <%= f.check_box :cellphone, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :cellphone, "Cell Phone" %>
              &ensp;
              <%= f.check_box :scanner, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :scanner, "Scanner" %>
              
          </div>
          <!-- Insert HR Checklist boxes here -->
          <div class="form-group well">
            <%= f.label :HRChecklist, "HR Checklist", class: 'strong' %>
            <br>
            <div class="form-check wrap">
              <div>
              <%= f.check_box :empStatusEmail, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :empStatusEmail, "Employment Status Email" %>
              </div>
              <div>
              <%= f.check_box :ADPComplete, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :ADPComplete, "ADP" %>
              </div>
              <div>
              <%= f.check_box :ITticket, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :ITticket, "IT Ticket submitted" %>
              </div>
              <div>
              <%= f.check_box :employeeFile, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :employeeFile, "Employee File Created" %>
              </div>
              <div>
              <%= f.check_box :bonus, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :bonus, "Bonus Structure" %>
              </div>
              <div>
              <%= f.check_box :drugTestRCVD, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :drugTestRCVD, "Drug Test Received" %>
              </div>
              <div>
              <%= f.check_box :review, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :review, "Review" %>
              </div>
              <div>
              <%= f.check_box :medical, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :medical, "Medical" %>
              </div>
              <div>
              <%= f.check_box :dental, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :dental, "Dental" %>
              </div>
              <div>
              <%= f.check_box :voluntary, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :voluntary, "Voluntary" %>
              </div>
              <div>
              <%= f.check_box :hsaBank, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :hsaBank, "HSA" %>
              </div>
              <div>
              <%= f.check_box :ncsRCVD, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :ncsRCVD, "NCS Received" %>
              </div>
              <div>
              <%= f.check_box :sapSubmit, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :sapSubmit, "SAP App Submitted" %>
              </div>
              <div>
              <%= f.check_box :vehicleInfo, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :vehicleInfo, "Vehicle Info" %>
              </div>
              <div>
              <%= f.check_box :eeo1Form, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :eeo1Form, "EEO-1 Form" %>
              </div>
              <div>
              <%= f.check_box :disability, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :disability, "Disability Self ID" %>
              </div>
              <div>
              <%= f.check_box :aap, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :aap, "AAP" %>
              </div>
              <div>
              <%= f.check_box :labCoat, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :labCoat, "Lab Coat Assigned" %>
              </div>
              <div>
              <%= f.check_box :locker, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :locker, "Locker Assigned" %>
              </div>
              <div>
              <%= f.check_box :trainingPapers, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :trainingPapers, "Training Papers" %>
              </div>
              <div>
              <%= f.check_box :emergencyList, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :emergencyList, "Emergency Contact List" %>
              </div>
              <div>
              <%= f.check_box :doorSystem, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :doorSystem, "Door System" %>
              </div>
              <div>
              <%= f.check_box :badge, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :badge, "ID Badge Printed" %>
              </div>
              <div>
              <%= f.check_box :traineeLog, {class: 'form-check-input'}, "yes", "no" %>
              <%= f.label :traineeLog, "Trainee Log" %>
              </div>
            </div>
          </div>
          
          <!-- Completed By -->
          <div class="form-group">
              <%= f.label :completedBy, "Completed By:" %>
              <%= f.text_field :completedBy, class: 'form-control'  %>
          </div>
      
          <%= f.submit 'Submit', class: 'btn btn-default' %>
        <% end %>
      </div>
    </div>
  </div>
</div>